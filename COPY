def using_concat(array, array)
  array.concat(array)
end

def using_insert(array, element)
array.insert(element)
end 

using_uniq(array)
aaray.uniq
end 

using_flatten(array[array]) 
array[array].flatten 
end 

using_delete(array, string)
array.delete(string)
end 

using_delete_at(array, integer)
array.delete_at(integer)
end